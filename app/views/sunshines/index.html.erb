<script>
window.onload = function() {
    if(typeof oldIE === 'undefined' && Object.keys)
        hljs.initHighlighting();

    baguetteBox.run('.baguetteBoxOne');
    baguetteBox.run('.baguetteBoxTwo');
    baguetteBox.run('.baguetteBoxThree', {
        animation: 'fadeIn'
    });
    baguetteBox.run('.baguetteBoxFour', {
        buttons: false
    });
    baguetteBox.run('.baguetteBoxFive', {
        captions: function(element) {
            return element.getElementsByTagName('img')[0].alt;
        }
    });
};


function contact_form_message_send(){
	var customer_name = $("#customer_name").val();
	var customer_email = $("#customer_email").val();
	var customer_message = $("#customer_message").val();
	$.ajax({
		type: "post",
		url: "/mail_send_to_customer",
		data: "name="+customer_name+"&email="+customer_email+"&message="+customer_message,
		success: function(msg){
			alert(msg);
			window.location.href="/";
		}
	});
} 

	function download_brochure(){
		$.ajax({
			type: "get",
			url: "/download_documentation", 
			success: function(msg){ 
				if(msg.trim() == ""){
					alert("Brochure File not available.");
					return;
				}
				url="/download_file?file_name="+encodeURIComponent(msg); 
				$("#iframe_download_div").html("");
				var iframe='<iframe name="file_download" id="file_download" style="display:none;" src="'+url +'"  ></iframe>'
				$("#iframe_download_div").html(iframe);   
			}
		});	 
	}
	
	function call_specific_gallery(type){
		$.ajax({
			type: "get",
			url: "get_gallery_type_wise_images?type="+type, 
			success: function(msg){ 
				img_arr = JSON.parse(msg);
				call_gallery_project_images(img_arr);
				call_gallery();
			}
		});	 
	}
	
	
	
	function dialog_alert(message) {
		$('#dialogMessage').html(message);
		$('#dialogAlert').fadeIn();
	}

	$('#closeDialogButton').click(function() {
		$('#dialogAlert').fadeOut();
	});
	
	function call_gallery(){
		window.location.href="#Gallery";
	}
	
	$(document).ready(function(){
		banner_load();
		var img_arr = <%= raw @image_arr.to_json %>;
		call_gallery_project_images(img_arr);
		baguetteBox.run('.baguetteBoxThree', {
			// Customize options as needed (if you have any)
		});
	});
	
	function call_gallery_project_images(image_arr){
		
		var html_content="<div class='grid baguetteBoxThree gallery'>";
		for (let i = 0; i < image_arr.length; i++) {
			html_content = html_content + "<div class='element-item transition' data-category='post-transition'>";
			html_content = html_content + "<a href='/project_images/"+image_arr[i]+"' target='_blank'>";
			html_content = html_content + "<div style='border: 2px solid #ff8400;'>";
			html_content = html_content + "<img src='/project_images/"+image_arr[i]+"' style='width: 407px;height: 307px;object-fit: cover;object-position: center center;filter: blur(10px) !important;'/> ";
			html_content = html_content + "</div>";
			html_content = html_content + "<div class='hover-img'>";
			html_content = html_content + "<i class='fa fa-camera camera'>S"+i+"</i>";
			html_content = html_content + "</div>";
			html_content = html_content + "</a>";
			html_content = html_content + "</div> ";
		}
		html_content = html_content + "</div> ";
		$("#gallery_div").html(html_content);
		baguetteBox.run('.baguetteBoxThree', {
			// Customize options as needed (if you have any)
		});
	}
	
	function banner_load(){
		$("body").css("overflow","hidden"); // Disable scrolling
		$('.banner-overlay').on('click', function() {
			$(this).fadeOut();
			$("body").css("overflow","visible"); // Enable scrolling
		});
	}

</script>
